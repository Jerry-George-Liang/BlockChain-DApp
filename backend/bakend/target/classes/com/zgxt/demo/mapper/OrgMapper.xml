<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zgxt.demo.mapper.OrgMapper">
    <insert id="add" parameterType="com.zgxt.demo.model.User" >
        insert into user(
            Id,username,orgType,address)
        values (#{Id},#{username},#{orgType},#{address})
    </insert>
</mapper>
